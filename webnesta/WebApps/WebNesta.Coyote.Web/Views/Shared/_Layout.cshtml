    <!-- Modal -->
    <!-- MODAL - SUCCESS -->
    <div class="modal fade modal-component" id="myModal" style="z-index:999999999999999999999999999 !important" role="dialog">
        <div class="modal-dialog">

            <div id="dvModalSuccess" style="height:auto !important;display:none;border-radius: 32px;padding: 15px;" class="modal-content popupAlertaFeedback">
                <div class="modal-header border-0 p-0 pr-2">
                    <button type="button" class="close text-white"
                            style="z-index: 99999999999999999999999;color: #c8c8c8;position: absolute;top: 18px; right: 30px; cursor: pointer;"
                            data-dismiss="modal">
                        &times;
                    </button>
                </div>
                <div class="modal-body">
                    <div class="row" style="display: table;">
                        <div class="col-3" style="display: table-cell;vertical-align: middle;">
                            <i class="fas fa-check-circle sucesso" style="margin-top:unset !important"></i>
                        </div>
                        <div class="col-9" style="padding-top: 8px;display: table-cell;vertical-align: middle;">
                            <p id="dvModalTitleSuccess" class="popupAlertaFeedback" style="font-size: 30px; font-weight: bold;margin-bottom: 16px;">
                                @WebNesta.Coyote.Web.App_GlobalResources.GlobalResource.ResourceManager.GetString("alert_success") !
                            </p>
                            <p id="dvModalMessageSuccess" class="popupAlertaFeedback"></p>
                        </div>
                    </div>
                </div>
            </div>

            <div id="dvModalError" style="height:auto !important;display:none;border-radius: 32px;" class="modal-content popupAlertaFeedback erro">
                <div class="modal-header border-0 p-0 pr-2">
                    <button type="button" class="close text-white"
                            style="z-index: 99999999999999999999999;
    color: black;
    position: absolute;
    top: 18px;
    right: 30px;
    cursor: pointer;"
                            data-dismiss="modal">
                        &times;
                    </button>
                </div>
                <div class="modal-body">
                    <div class="row" style="display: table;">
                        <div class="col-3" style="display: table-cell;vertical-align: middle;">
                            <i class="fas fa-times-circle erro" style="margin-top:unset !important"></i>
                        </div>
                        <div class="col-9" style="padding-top: 8px;display: table-cell;vertical-align: middle;">
                            <p id="dvModalTitleError" class="popupAlertaFeedback" style="font-size: 30px; font-weight: bold;margin-bottom: 16px;">Oops</p>
                            <p id="dvModalMessageError" class="popupAlertaFeedback"></p>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>

    <!-- MODAL - DECISION -->
    <div class="modal fade modal-component" id="myModalDecision" style="z-index:999999999999999999999999999 !important" role="dialog">
        <div class="modal-dialog">

            <div id="dvModalDecision" style="height:auto !important;display:none;border-radius: 32px;" @*class="modal-content popupAlertaFeedback erro"*@>
                <div class="modal-header border-0 p-0 pr-2">
                    <button type="button" class="close text-white"
                            style="z-index: 99999999999999999999999;color:#c8c8c8;position:absolute;top:18px;right:30px;cursor:pointer;"
                            data-dismiss="modal">
                        &times;
                    </button>
                </div>
                <div class="modal-body">
                    <div class="row" style="display: table;">
                        <div class="col-3" style="display: table-cell;vertical-align: middle;">
                            <i class="fas fa-times-circle erro" style="margin-top:unset !important"></i>
                        </div>
                        <div class="col-9" style="padding-top: 8px;display: table-cell;vertical-align: middle;">
                            <p id="dvModalTitleDecision" class="popupAlertaFeedback" style="font-size: 30px; font-weight: bold;margin-bottom: 16px;">Oops</p>
                            <p id="dvModalMessageDecision" class="popupAlertaFeedback"></p>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-12 text-center" style="margin-top: 3%;">
                            <input type="button" class="btn_login text-center" 
                                name="@WebNesta.Coyote.Web.App_GlobalResources.GlobalResource.ResourceManager.GetString("btn_ok")" 
                                value="@WebNesta.Coyote.Web.App_GlobalResources.GlobalResource.ResourceManager.GetString("btn_ok")" 
                            id="btnOkDecision" />
                            <input type="button" class="btn_login text-center" style="margin-left: 24px;"
                                   name="@WebNesta.Coyote.Web.App_GlobalResources.GlobalResource.ResourceManager.GetString("btn_cancelar")"
                                   value="@WebNesta.Coyote.Web.App_GlobalResources.GlobalResource.ResourceManager.GetString("btn_cancelar")"
                                   id="btnCancelDecision" />
                                   <input type="hidden" id="hfDecisionId"/>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>


