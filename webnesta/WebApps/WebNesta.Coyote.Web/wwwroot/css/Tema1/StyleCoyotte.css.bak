.home-section {
    position: relative;
    background: #F5F5F5;
    min-height: 94vh;
    max-height: 94vh;
    overflow: auto;
    top: 0;
    left: 78px;
    width: calc(100% - 78px);
    transition: all 0.5s ease;
    z-index: 2;
    border-radius: 42px;
    margin: 20px 20px 0px 0px;
}

