.help {
    color: #E1E1E1;
    font-size: 1em !important;
    cursor: pointer;
    margin: auto 0;
}

    .help .tooltip {
        visibility: hidden;
        position: fixed;
        opacity: 0.9;
        left: 10%;
        width: 120px;
        background-color: #808080;
        border: 1px solid #6c6868;
        color: #fff;
        text-align: left;
        padding: 5px;
        border-radius: 6px;
        z-index: 120000;
        transition: opacity .6s;
        font-size: 10pt;
        line-height: 12pt;
        font-family: 'Myriad_Web_Pro';
    }

    .help:hover .tooltip {
        visibility: visible;
        z-index: 120000;
    }

