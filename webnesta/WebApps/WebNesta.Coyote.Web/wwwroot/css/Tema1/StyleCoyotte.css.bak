.button {
    display: block;
    width: 50px;
    height: 50px;
    color: #959595;
    border: 1px solid #C8C8C8;
    border-radius: 13px;
    text-align: center;
    position: relative;
}

    .button i {
        margin: 0;
        position: absolute;
        top: 50%;
        left: 50%;
        margin-right: -50%;
        transform: translate(-50%, -50%);
        font-size: 18px;
    }

    .button:hover {
        background: #3B95E5;
        border: 1px solid #fff;
        color: #fff;
    }

    .button .tooltip {
        position: absolute;
        top: -20px;
        left: calc(100% + 5px);
        z-index: 3;
        background: #3B95E5;
        box-shadow: 0 12px 35px rgba(55, 77, 98, 0.25);
        padding: 6px 12px;
        border-radius: 5px;
        font-size: 15px;
        font-weight: 400;
        opacity: 0;
        white-space: nowrap;
        pointer-events: none;
        transition: 0s;
        color: #fff;
    }

    .button:hover .tooltip {
        position: absolute;
        opacity: 1;
        pointer-events: auto;
        transition: all 0.4s ease;
        top: 50%;
        transform: translateY(-50%);
    }

    .button.open .tooltip {
        display: none;
    }

    .button .tooltip-b {
        position: absolute;
        /*top: -20px;*/
        left: calc(0% + 5px);
        z-index: 3;
        background: #3B95E5;
        box-shadow: 0 12px 35px rgba(55, 77, 98, 0.25);
        padding: 6px 12px;
        border-radius: 4px;
        font-size: 15px;
        font-weight: 400;
        opacity: 0;
        white-space: nowrap;
        pointer-events: none;
        transition: 0s;
        color: #fff;
    }

    .button:hover .tooltip-b {
        position: absolute;
        opacity: 1;
        pointer-events: auto;
        transition: all 0.4s ease;
        top: calc(100% + 20px);
        left: 0;
        transform: translateY(-50%);
    }

